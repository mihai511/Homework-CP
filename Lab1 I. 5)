#include <iostream>
#include <cmath>
using namespace std;
int n1,n2,i,r1,r2;
long long p;
int main(){
    cin>>n1>>n2;
    cout<<"square root of n1: "<<sqrt(n1)<<"\n";
    cout<<"square root of n2: "<<sqrt(n2)<<"\n";
    p=1;
    for(i=1;i<=n2;i++)
        p=p*n1;
    cout<<"n1 to the power of n2: "<<p<<"\n";
        r1=0;
    while(n1!=0){
        r1=r1*10+n1%10;
        n1=n1/10;
    }
    cout<<"n1 inversed: "<<r1<<"\n";
    r2=0;
    while(n2!=0){
        r2=r2*10+n2%10;
        n2=n2/10;
    }
    cout<<"n2 inversed: "<<r2<<"\n";
    return 0;
}
